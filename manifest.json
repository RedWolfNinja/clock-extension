{
  "name": "clock-extension",
  "short_name": "Clock on new tab",
  "version": "1.0",
  "description": "Replaces chrome://newtab with analogue clock",
  "manifest_version": 2,
  "chrome_url_overrides" : {
    "newtab": "background.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
